{
  "multipart": [
    {
      "when": {
        "straight_x": "true"
      },
      "apply": {
        "model": "pepro:block/conduit_straight",
        "y": 90
      }
    },
    {
      "when": {
        "straight_y": "true"
      },
      "apply": {
        "model": "pepro:block/conduit_straight",
        "x": 90
      }
    },
    {
      "when": {
        "straight_z": "true"
      },
      "apply": {
        "model": "pepro:block/conduit_straight"
      }
    },
    {
      "when": {
        "hub": "true",
        "straight_x": "false",
        "straight_y": "false",
        "straight_z": "false"
      },
      "apply": {
        "model": "pepro:block/conduit_hub"
      }
    },
    {
      "when": {
        "north": "true",
        "straight_x": "false",
        "straight_y": "false",
        "straight_z": "false"
      },
      "apply": {
        "model": "pepro:block/conduit_connector",
        "x": 90
      }
    },
    {
      "when": {
        "east": "true",
        "straight_x": "false",
        "straight_y": "false",
        "straight_z": "false"
      },
      "apply": {
        "model": "pepro:block/conduit_connector",
        "x": 90,
        "y": 90
      }
    },
    {
      "when": {
        "south": "true",
        "straight_x": "false",
        "straight_y": "false",
        "straight_z": "false"
      },
      "apply": {
        "model": "pepro:block/conduit_connector",
        "x": 90,
        "y": 180
      }
    },
    {
      "when": {
        "west": "true",
        "straight_x": "false",
        "straight_y": "false",
        "straight_z": "false"
      },
      "apply": {
        "model": "pepro:block/conduit_connector",
        "x": 90,
        "y": 270
      }
    },
    {
      "when": {
        "up": "true",
        "straight_x": "false",
        "straight_y": "false",
        "straight_z": "false"
      },
      "apply": {
        "model": "pepro:block/conduit_connector"
      }
    },
    {
      "when": {
        "down": "true",
        "straight_x": "false",
        "straight_y": "false",
        "straight_z": "false"
      },
      "apply": {
        "model": "pepro:block/conduit_connector",
        "x": 180
      }
    }
  ]
}